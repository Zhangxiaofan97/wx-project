
.slam {
  display: block;
  width: 100vw;
  height: 100vh;
}

.buttonBox{
  width: 310rpx;
  height: 116rpx;
  position: absolute;
  bottom: 152rpx;
  left: 50%;
  transform: translate(-50%, 0%);
  border-radius: 40px;
  overflow: hidden;
  border: 4rpx solid #ffffff;
  z-index: 9999;
}

.buttonBox .shade{
  width: 310rpx; 
  height: 116rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
  background-color: rgba(0, 0, 0, 0.5);
}

.buttonBox .bgWave{
  width: 100%;
  height: 100%;
  z-index: 1;
}

.buttonBox .shell{
  width: 96rpx;
  height: 96rpx;
  position: absolute;
  top: 50%;
  right: 36rpx;
  transform: translate(0%, -50%);
  z-index: 9999;
}

.buttonBox .text{
  width: 238rpx;
  height: 84rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 30rpx;
  position: absolute;
  z-index: 9999;
  top: 50%;
  left: 44rpx;
  transform: translate(0%, -50%);
  display: grid;
}

.textBox{
  width: 610rpx;
  height: 122rpx;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 12rpx;
  position: absolute;
  top: 224rpx;
  left: 50%;
  transform: translate(-50%, 0%);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 99999;
}

.textBox text{
  color: #FFFFFF;
  font-size: 34rpx;
}

.knot{
  width: 152rpx;
  height: 152rpx;
  position: absolute;
  bottom: 310rpx;
  left: 50%;
  transform: translate(-50%, 0%);
  z-index: 9999;
}

.popUpWindows{
  width: 640rpx;
  height: 360rpx;
  background-color: #FFFFFF;
  border-radius: 16rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 9999999;
}

.popUpWindows .popUpWindowsText1{
  width: 100%;
  color: #353535;
  font-size: 34rpx;
  text-align: center;
  margin-bottom: 6rpx;
}

.popUpWindows .popUpWindowsText2{
  width: 100%;
  color: #E61C1C;
  font-size: 34rpx;
  text-align: center;
  margin-bottom: 6rpx;
}

.popUpWindows .popUpWindowsButton{
  width: 100%;
  height: 110rpx;
  color: #576B95;
  font-size: 34rpx;
  text-align: center;
  line-height: 110rpx;
  position: absolute;
  bottom: 0;
  border-top: 1px solid #E5E5E5;
}

.backButton {
	width: 174rpx;
	height: 64rpx;
	position: absolute;
	top: 102rpx;
	left: 12rpx;
	background-color: rgba(255, 255, 255, 0.60);
	z-index: 9999999;
	border-radius: 50rpx;
}
.backButton .back {
	width: 22rpx;
	height: 36rpx;
	position: absolute;
	left: 34rpx;
	top: 50%;
	-webkit-transform: translate(0%, -50%);
	        transform: translate(0%, -50%);
}
.backButton .backBox{
	width: 88rpx;
	height: 64rpx;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 9999999;
}
.backButton .segmentation {
	width: 1rpx;
	height: 37rpx;
	background-color: rgba(0, 0, 0, 0.20);
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
}
.backButton .backHome {
	width: 46rpx;
	height: 44rpx;
	position: absolute;
	right: 20rpx;
	top: 50%;
	-webkit-transform: translate(0%, -50%);
	        transform: translate(0%, -50%);
}
.backButton .backHomeBox{
	width: 84rpx;
	height: 64rpx;
	position: absolute;
	top: 0;
	right: 0;
	z-index: 9999999;
}


  