
/* pages/wxCase/customHead/index.wxss */
page {
  width: 100%;
}

.header {
  width: 100%;
  position: relative;
  height: 500rpx;
}

.header .header-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.head-back {
  padding: 0 20rpx;
  box-sizing: border-box;
  height: 50rpx;
  width: 70rpx;
  margin-left: 25rpx;
  position: relative;
}

.head-back image {
  width: 40rpx;
  height: 40rpx;
}

.container12{
  width: 100%;
  height: 1000rpx;
  padding:100rpx 0rpx;
  background-color: rgb(85, 93, 99);
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.button1{
  width: 550rpx;
  height: 220rpx;
  background-color: white;
  padding: 40rpx 35rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  border-radius: 30rpx;
  margin-bottom: 40rpx;
}
.button1Image{
  width: 150rpx;
  height: 150rpx;
}

.buttonwenzi{
  display: flex;
  flex-direction: column;
}
.buttonwenziTitle{
  font-size: 80rpx;
}
.buttonwenziText{
  font-size: 40rpx;
}
.container34{
  width: 100%;
  height: 465rpx;
  padding: 0rpx 45rpx;
  background-color: rgb(181, 200, 213);
  display: flex;
  flex-direction: column;
}
.container34Title{
  font-size: 40rpx;
  font-weight: bold;
}
.container34Img{
  margin-top: 20rpx;
  height: 250rpx;
  width: 650rpx;
  border-radius: 25rpx;
}
